<h1> Help A Dog Find A Home: </h1>

<ul>
<% @dog.errors.full_messages.each do |message| %>
    <li>
        <%= message %>
    </li>
<% end %>
</ul>
<%= form_for @dog do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label "Image" %>
    <%= f.text_field :img %>
    <%= f.submit %>
    <% end %>